{
  "name": "farm-manager-ui",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "dev:watch": "npx concurrently \"npm run codegen:get-schema:watch\"  \"npm run dev\"",
    "codegen": "npx graphql-codegen --config codegen.ts",
    "codegen:watch": "npx graphql-codegen --config codegen.ts --watch",
    "get-schema": "node get-schema.js > schema.graphql",
    "codegen:get-schema": "npm run get-schema && npm run codegen",
    "codegen:get-schema:watch": "npm run get-schema && npm run codegen:watch"
  },
  "dependencies": {
    "@apollo/client": "^3.13.5",
    "@graphql-codegen/cli": "^5.0.5",
    "@graphql-codegen/typescript-graphql-files-modules": "^3.0.0",
    "@hookform/resolvers": "^4.1.3",
    "@parcel/watcher": "^2.5.1",
    "@radix-ui/react-checkbox": "^1.1.5",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-slot": "^1.1.2",
    "@radix-ui/react-tabs": "^1.1.3",
    "axios": "^1.8.4",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.7.3",
    "graphql": "^16.10.0",
    "graphql-ws": "^6.0.4",
    "lucide-react": "^0.483.0",
    "luxon": "^3.6.1",
    "next": "15.2.3",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.54.2",
    "recharts": "^2.15.1",
    "sonner": "^2.0.1",
    "tailwind-merge": "^3.0.2",
    "tw-animate-css": "^1.2.4",
    "zod": "^3.24.2",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/date-fns": "^2.5.3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.2.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
